generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model paciente{ 
  id                      Int      @id @unique @default(autoincrement())
  nome                    String  
  dataNascimento          String
  genero                  String
  telefone                Int
  email                   String
  senha                   String
}

model locais {
  id       String @id @unique
  nome     String
  endereco String
  telefone Int
}

model procedimentos{
  id                      String       @id @unique
  nome                    String
  descricao               String
  duracao                 String
}

model profissionais{
id                        String       @id @unique
nome                      String
especialidade             String
}

model locais_procedimentos{
  id                      String       @id @unique
  locais_id               String
  procediemntos_id        String
}

model profissionais_procedimentos{
  id                      String      @id @unique
  profissionais_id        String
  procedimentos_id        String

}
model agendamento{
  id                      String      @id @unique
  data                    DateTime
  hora                    DateTime
  descricao               String

}

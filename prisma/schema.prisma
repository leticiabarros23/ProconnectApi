// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model paciente{ 
  id                      String      @id @unique 
  nome                    String  
  dataNascimento          String
  genero                  String
  telefone                Int
  email                   String
  senha                   String
}

model locais{
  id                      String      @id @unique
  nome                    String
  endereco                String
  telefone                Int
}

model procedimentos {
  id                      String       @id @unique
  nome                    String
  descricao               String
  duracao                 String
}

model profissionais {
id                        String       @id @unique
nome                      String
especialidade             String
}

model agendamento{
  id                      String      @id @unique
  data                    DateTime
  hora                    DateTime
  descricao               String

}
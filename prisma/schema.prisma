generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model paciente {
  id             String @id @unique
  nome           String
  dataNascimento String
  email          String
  genero         String
  senha          String
  telefone       Int
}

model locais {
  id       String @id @unique
  nome     String
  endereco String
  telefone Int
}

model procedimentos {
  id        String @id @unique
  nome      String
  descricao String
  duracao   String
}

model profissionais {
  id            String @id @unique
  nome          String
  especialidade String
}

model agendamento {
  id        String   @id @unique
  data      DateTime
  hora      DateTime
  descricao String
}
